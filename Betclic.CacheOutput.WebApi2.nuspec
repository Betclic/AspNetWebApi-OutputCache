<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Betclic.CacheOutput.WebApi2</id>
    <version>0.9.0</version>
    <title>Betclic.CacheOutput.WebApi2</title>
    <authors>Filip W</authors>
    <licenseUrl>https://github.com/filipw/AspNetWebApi-OutputCache/blob/master/License.txt</licenseUrl>
    <projectUrl>https://github.com/filipw/AspNetWebApi-OutputCache</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A library bringing output caching (similar to MVC's "OutputCache"), to Web API actions. Strathweb.CacheOutput will take care of server side caching and set the appropriate client side (response) headers for you.
      This is a version for Web Api 2 only.
    </description>
    <releaseNotes />
    <copyright>2013-2015 Filip W</copyright>
    <language>en-CA</language>
    <tags>aspnet, webapi, aspnetwebapi</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Microsoft.AspNet.WebApi.Core" version="5.2.3" />
        <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.3" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Runtime.Caching" targetFramework=".NETFramework4.0" />
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="src\WebApi.OutputCache.V2\bin\Release\WebApi.OutputCache*.*" target="lib\net45" />
  </files>
</package>